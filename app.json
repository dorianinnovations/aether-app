{
  "expo": {
    "name": "AetheR",
    "slug": "AetheR",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "aether",
    "plugins": [
      [
        "expo-auth-session",
        {
          "schemes": ["aether"]
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.isaiahpappas.aether",
      "buildNumber": "24",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera to take profile and banner photos.",
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library to select profile and banner images."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7cecb5c5-d560-4cb6-bb9d-850e14d66014"
      }
    },
    "owner": "dorianinnovations"
  }
}
